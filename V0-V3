#include "stdio.h"
#include "string.h"
int main(){
	char ch[]="1+2+2+1+2+5+4-1-3+4-8";
	int sum=ch[0]-'0';
	for(int i=1;i<strlen(ch);i++)
	{
		if(ch[i]=='+')
		{
			int a=ch[i+1]-'0';
		    sum=sum+a;
		    i++;
		}
		else if(ch[i]=='-')
		{
			int b=ch[i+1]-'0';
			sum=sum-b;
			i++;
		}
	}
	printf("%d\n",sum);
	return 0;
}
#include "stdio.h"
#include "string.h"
int main()
{
	char ch[]="2*2/4*1/1*2*3/2";
	int s=ch[0]-'0';
	for(int i=1;i<strlen(ch);i++)
	{
		if(ch[i]=='*')
		{
			int a=ch[i+1]-'0';
		    s=s*a;
		    i++;
		}
		else if(ch[i]=='/')
		{
			int b=ch[i+1]-'0';
			s=s/b;
			i++;
		}
	}
	printf("%d\n",s);
	return 0;
}
#include"stdio.h"
#include"string.h"
int main()
{
	char ch[30];
	gets_s(ch);
	char ch1[30];
	int left, right, m = -1;
	for (int i = 0; i < strlen(ch); i++)
	{
		if (ch[i] == '*')
		{
			left = ch1[m] - '0';
			right = ch[i + 1] - '0';
			ch1[m] = left * right + '0';
			i++;
		}
		else if (ch[i] == '/')
		{
			left = ch1[m] - '0';
			right = ch[i + 1] - '0';
			ch1[m] = left / right + '0';
			i++;
		}
		else ch1[++m] = ch[i];
	}
	int s = ch1[0] - '0';
	for (int i = 1; i < strlen(ch1); i++)
	{
		if (ch1[i] == '+')
			s = s + (ch1[++i] - '0');
		else if (ch1[i] == '-')
			s = s - (ch1[++i] - '0');
	}
	printf("%d\n", s);

}
